
@{
    ViewBag.Title = "UploadIcons";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

@using (Html.BeginForm("UploadIcons", "App",FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="panel panel-success">
        <div class="panel-heading">
            <strong>Upload Images</strong>
        </div>
        <div class="panel-body">

            <div class="pmbb-edit" id="conedit">
                <dl class="dl-horizontal">
                    <dt class="p-t-10">Images</dt>
                    <dd>
                        <div class="fg-line">

                            <input type="file" name="file" accept="image/x-png,image/jpeg" /> 
                        </div>
                    </dd>


                </dl>

                

            </div>
        </div>
        <div class="panel-footer">
            <div class="col-lg-offset-1">
                <input type="submit" class="btn-primary" value="Upload" />
            </div>
            
        </div>
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
